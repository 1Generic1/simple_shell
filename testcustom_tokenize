#include <stdio.h>
#include <stdlib.h>
#include "shell.h"

#define MAX_ARGS 10

char **custom_tokenize(char *cmd);

int main(void)
{
    char cmd1[] = "ls";
    char cmd2[] = "ls -l";
    char cmd3[] = "echo Hello World";

    char **tokens1 = custom_tokenize(cmd1);
    char **tokens2 = custom_tokenize(cmd2);
    char **tokens3 = custom_tokenize(cmd3);

printf("Tokenized cmd1:\n");
    for (int i = 0; tokens1[i] != NULL; i++)
    {
        printf("Token %d: %s\n", i, tokens1[i]);
    }

    printf("Tokenized cmd2:\n");
    for (int i = 0; tokens2[i] != NULL; i++)
    {
        printf("Token %d: %s\n", i, tokens2[i]);
    }

    printf("Tokenized cmd3:\n");
    for (int i = 0; tokens3[i] != NULL; i++)
    {
        printf("Token %d: %s\n", i, tokens3[i]);
    }

    // Free the allocated memory
    for (int i = 0; tokens1[i] != NULL; i++)
    {
        free(tokens1[i]);
    }
    free(tokens1);

    for (int i = 0; tokens2[i] != NULL; i++)
    {
        free(tokens2[i]);
    }
    free(tokens2);

    for (int i = 0; tokens3[i] != NULL; i++)
    {
        free(tokens3[i]);
    }
    free(tokens3);

    return (0);
}

